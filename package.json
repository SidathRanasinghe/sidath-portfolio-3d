    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "embla-carousel-autoplay": "^8.6.0",
    "embla-carousel-react": "^8.6.0",
    "gsap": "^3.13.0",
    "lucide-react": "^0.511.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-responsive": "^10.0.1",
    "tailwind-merge": "^3.3.0",
    "tailwindcss": "^3.4.17",
    "three": "^0.169.0",
    "tw-animate-css": "^1.3.0"
